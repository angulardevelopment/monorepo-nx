    "frontend/store-admin-e2e": {
      "tags": []
    },
    "frontend-store-admin": {
      "tags": []
    },
    "demo-e2e": {
      "tags": []
    },
    "demo": {
      "tags": []
    },
    "myapp-e2e": {
      "tags": []
    },
    "myapp": {
      "tags": []
    },
    "mylib": {
      "tags": []
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
